{
  "compilerOptions": {
    "lib": ["ES2016", "ES2017", "ES2018", "ES2019", "ES2020", "ESNext", "DOM"],
    "outDir": "./dist/",
    "declaration": true,
    "strict": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "target": "ES6",
    "allowJs": false,
    "importHelpers": true,
    "types": []
  },
  "include": ["./src/**/*"]
}
